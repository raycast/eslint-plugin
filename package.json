{
  "name": "@raycast/eslint-plugin",
  "version": "1.0.5",
  "description": "ESLint plugin designed to help Raycast's extensions authors follow best practices",
  "author": "Raycast Technologies Ltd.",
  "keywords": [
    "raycast",
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "type:check": "tsc --noEmit",
    "test": "jest",
    "test:ci": "jest --ci",
    "preupdate:docs": "npm run build",
    "update:docs": "eslint-doc-generator",
    "lint": "npm-run-all \"lint:*\"",
    "lint:eslint-docs": "npm-run-all \"update:docs --check\"",
    "lint:ts": "eslint ."
  },
  "dependencies": {
    "@typescript-eslint/utils": "^5.48.1",
    "title-case": "^3.0.3"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.19.0",
    "eslint-doc-generator": "^1.4.2",
    "eslint-plugin-eslint-plugin": "^5.0.7",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^4.0.4",
    "ts-jest": "^29.0.5"
  },
  "peerDependencies": {
    "eslint": ">=7"
  }
}
